{
  "name": "express-server",
  "version": "0.0.1",
  "license": "",
  "main": "dist/server.js",
  "engines": {
    "node": "22.x",
    "npm": "10.x"
  },
  "scripts": {
    "build": "tsc && cp -r *.json dist/",
    "start": "node dist/server.js",
    "start:mac": "tsc && cp -r *.json dist/ && node dist/server.js",
    "start:windows": "tsc && xcopy *.json dist\\ /E /I && node dist\\server.js"
  },
  "dependencies": {
    "@types/node": "18.11.9",
    "body-parser": "1.20.1",
    "cors": "2.8.5",
    "express": "4.18.2",
    "qs": "6.11.0",
    "raw-body": "2.5.1",
    "cookie": "0.5.0",
    "encodeurl": "1.0.2",
    "finalhandler": "1.2.0",
    "merge-descriptors": "1.0.1",
    "path-to-regexp": "0.1.7",
    "send": "0.18.0",
    "serve-static": "1.15.0"
  },
  "devDependencies": {
    "typescript": "4.8.4"
  }
}
