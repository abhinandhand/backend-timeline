{
  "version": 2,
  "builds": [
    {
      "src": "dist/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "methods": ["OPTIONS", "GET", "POST"],
      "dest": "dist/server.ts",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/(.*)",
      "methods": ["OPTIONS", "GET", "POST"],
      "dest": "dist/server.ts",
       "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
