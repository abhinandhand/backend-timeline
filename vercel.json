{
  "version": 2,
  "builds": [
    {
      "src": "dist/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/transactions",
      "methods": ["OPTIONS", "GET", "POST"],
      "dest": "dist/server.js"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/api/transactions"
    }
  ]
}
