{
  "version": 2,
  "builds": [
    {
      "src": "server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "methods": ["OPTIONS", "GET", "POST"],
      "dest": "server.ts",
       "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
